{
      "name": "Moodle-Skip-Login",
      "version": "1.0.0",
      "description": "An extension to automatically skip the login page on Moodle",
      "manifest_version": 3,
      "author": "JoaoAJMatos",
      "action":{
          "default_popup": "src/index.html",
          "default_title": "Moodle Skip Login"
      },
      "background": {
          "service_worker": "src/index.js"
      },
      "permissions": [
          "tabs",
          "scripting",
          "storage"
      ],
      "host_permissions": [
          "https://ead.ipleiria.pt/*"
      ]
  }